    <script>
        function displayToastMessage(title, message, type) {
            // Set up Toastr options
            var options = {
                closeButton: true,
                progressBar: true,
                timeOut: 8000, // Set the duration of the toast
                extendedTimeOut: 1000, // Set the extended duration for when the toast is hovered
                onclick: null,
                positionClass: 'toast-top-right',
                preventDuplicates: false, // Allow multiple toasts
                // Specify custom CSS class for light color theme
                toastClass: 'toastr-light',
            };
            // Show Toastr
            var toastrInstance = toastr[type](message, title, options);

            // Add progress number
            var progressBar = $(toastrInstance).find('.toast-progress');

            var progress = 0;
            var interval = setInterval(function () {
                progress += 1;
                progressBar.attr('value', progress);
                if (progress >= 100) {
                    clearInterval(interval);
                }
            }, (options.timeOut + options.extendedTimeOut) / 100); // Increment progress based on timeOut and extendedTimeOut
        }

 @*displayToastMessage('@ViewBag.Message', 'Error', 'error');*@
    </script>
